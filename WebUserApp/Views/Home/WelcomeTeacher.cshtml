                        <div id="world-map-markers" style="height: 325px; overflow: hidden">

                            <div class="map-responsive"><iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d12497.85463226484!2d27.121750072266206!3d38.45385580772181!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14bbd9c9f1050713%3A0x829b467f568102f7!2sKarsiyaka%20Evlendirme%20Dairesi!5e0!3m2!1sen!2str!4v1623239804728!5m2!1sen!2str" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe></div>
                        </div>
                    </div>
                </div><!-- /.d-md-flex -->
            </div>
            <!-- /.card-body -->
        </div>
        <!-- /.col -->
    </div>
            <!-- /.row -->

</section>
