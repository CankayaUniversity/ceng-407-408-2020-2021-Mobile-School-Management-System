                    <div class="card-header">
                        <h3 class="card-title">Öğretmenler</h3>

                        <div class="card-tools">
                            <span class="badge badge-danger">@Model.TeacherList.Count Adet Öğretmen </span>
                            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                <i class="fas fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-tool" data-card-widget="remove">
                                <i class="fas fa-times"></i>
                            </button>
                        </div>
                    </div>
                    <!-- /.card-header -->
                    @foreach (var item in Model.TeacherList)
                    {
                        <div class="card-body p-0">
                            <ul class="users-list clearfix">
                                <li>
                                    <img src="~/dist/img/user1-128x128.jpg" alt="User Image">
                                    <a class="users-list-name" href="#">@item.FullName</a>
                                    <span class="users-list-date">@item.Branch.BranchName</span>
                                </li>
                                
                            </ul>
                            <!-- /.users-list -->
                        </div>
                        <!-- /.card-body -->
                    }

                    <div class="card-footer text-center">
                        <a href="/Student/Teachers">Tüm Öğretmenleri Gör</a>
                    </div>
                    <!-- /.card-footer -->
                </div>
                <!--/.card -->
            </div>
            <!-- /.col -->
            <div class="col-md-4">
                <!-- Info Boxes Style 2 -->
                <div class="info-box mb-3 bg-warning">
                    <span class="info-box-icon"><i class="fas fa-tag"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">En Yakın Sınav</span>
                        <div class="row">
                            <span class="info-box-number  col-md-8">@Model.MostRecentExam</span>
                            <a href="/Student/Exams" class="btn btn-sm btn-secondary float-right col-md-4">Tümünü Gör</a>
                        </div>

                        
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
                <div class="info-box mb-3 bg-success">
                    <span class="info-box-icon"><i class="far fa-heart"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">Kalan Sınav Adeti</span>
                        <div class="row">
                            <span class="info-box-number col-md-8">@Model.ExamsLeft</span>
                            <a href="/Student/Exams" class="btn btn-sm btn-secondary float-right col-md-4">Tümünü Gör</a>
                        </div>

                        
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
                
                <!-- /.info-box -->
                <div class="info-box mb-3 bg-info">
                    <span class="info-box-icon"><i class="far fa-comment"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">Mesajlar</span>
                        <div class="row">
                            <span class="info-box-number  col-md-8">@Model.TotalMessages</span>
                            <a href="/Message/Mailbox" class="btn btn-sm btn-secondary float-right col-md-4">Tümünü Gör</a>
                        </div>
                       
                    </div>
                    <!-- /.info-box-content -->
                </div>
            </div>

        </div>



        <!-- /.info-box -->
        <!-- PRODUCT LIST -->
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Duyurular</h3>

                <div class="card-tools">
                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                    <button type="button" class="btn btn-tool" data-card-widget="remove">
                        <i class="fas fa-times"></i>
                    </button>
                </div>
            </div>
            <!-- /.card-header -->
            <div class="card-body p-0">
                <ul class="products-list product-list-in-card pl-2 pr-2">
                   
                        @foreach (var item in Model.NewsList)
                        {
                         <li class="item">
                             <div class="product-info">
                                 <a class="product-title">
                                        @item.Title
                                     <span class="badge badge-warning float-right">@item.NewsDate</span>
                                 </a>
                                   <span class="product-description">
                                     @item.NewsContent
                                  </span>
                             </div>

                         </li>
                        }
                        
                    <!-- /.item -->
                </ul>
            </div>
            <!-- /.card-body -->

            <div class="card-footer text-center">
                <a href="/Student/Announcements" class="uppercase">Tüm Duyuruları Gör</a>
            </div>
            <!-- /.card-footer -->
            <!-- /.card -->
        </div>
        <!-- MAP & BOX PANE -->
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Okul Konum</h3>

                <div class="card-tools">
                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                    <button type="button" class="btn btn-tool" data-card-widget="remove">
                        <i class="fas fa-times"></i>
                    </button>
