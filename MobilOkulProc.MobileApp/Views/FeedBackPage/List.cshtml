@using MobilOkulProc.Entities.Concrete
@using MobilOkulProc.MobileApp.Models
@model FeedbackPageModel<FEEDBACK>

@{
    ViewData["Title"] = "List Feedback";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table">
    <thead>
        
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Mesajlar.Nesne.FeedbackContent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mesajlar.Nesne.FeedbackDate)
            </th>

            
        </tr>
        
        </thead>
        
    <tbody>
        @foreach (var item in Model.PagedList)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FeedbackContent)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.FeedbackDate)
            </td>

        </tr>
        }
    </tbody>
</table>






